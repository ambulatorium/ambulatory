{
    "name": "reliqui/ambulatory",
    "description": "An open source ambulatory care platform",
    "keywords": ["laravel", "outpatient care", "ambulatory care", "scheduling appointment"],
    "license": "MIT",
    "authors": [
        {
            "name": "David H. Sianturi",
            "email": "davidhsianturi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.7",
        "rlanvin/php-rrule": "^2.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.3.15",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Reliqui\\Ambulatory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reliqui\\Ambulatory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reliqui\\Ambulatory\\AmbulatoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
